    gem "jekyll-default-layout", "~> 0.1"
    gem "jekyll-gist"
    gem "jekyll-feed", "~> 0.9"
    gem "jekyll-algolia"
    gem "jemoji", "~> 0.9"
    gem "jekyll-remote-theme", "~> 0.3"
